{
  "name": "@leancodepl/nx-plugins",
  "version": "9.7.3",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./intl": {
      "types": "./dist/intl.d.ts",
      "import": "./dist/intl.js",
      "default": "./dist/intl.js"
    },
    "./proxy": {
      "types": "./dist/proxy.d.ts",
      "import": "./dist/proxy.js",
      "default": "./dist/proxy.js"
    },
    "./contracts": {
      "types": "./dist/contracts.d.ts",
      "import": "./dist/contracts.js",
      "default": "./dist/contracts.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leancodepl/js_corelibrary.git",
    "directory": "packages/nx-plugins"
  },
  "homepage": "https://github.com/leancodepl/js_corelibrary",
  "bugs": {
    "url": "https://github.com/leancodepl/js_corelibrary/issues"
  },
  "description": "Collection of Nx plugins for LeanCode projects",
  "keywords": [
    "nx",
    "nx-plugin",
    "generators",
    "executors",
    "typescript",
    "leancode"
  ],
  "author": {
    "name": "LeanCode",
    "url": "https://leancode.co"
  },
  "dependencies": {
    "@nx/devkit": "^21.0.0",
    "tslib": "^2.8.1"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "!**/*.tsbuildinfo"
  ]
}
